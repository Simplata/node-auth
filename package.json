{
  "name": "community-backend",
  "version": "1.0.0",
  "description": "Community backend server. ExpressJS.",
  "repository": "https://github.com/ali-wetrill/community-backend",
  "author": "Ali Ahmed <ali@simplata.io>",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "eslint"
  ],
  "dependencies": {
    "async": "^3.1.0",
    "bcrypt": "^1.0.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "express": "^4.16.2",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^7.2.1",
    "lusca": "^1.6.1",
    "mongoose": "^5.7.5",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "nodemon": "^1.19.3",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  }
}
